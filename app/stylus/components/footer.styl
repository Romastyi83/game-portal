.footer
  position relative
  +element(inner)
    height 95px
    display flex
    justify-content space-between
    align-items center
.copyright
  font-weight 400
  font-size 11px
  letter-spacing -0.05em
  color #7A7A7A
  +element(holder)
    display flex
    align-items center
  +element(info)
    margin-left 12px
  +element(link)
    font-weight 400
    font-size 11px
    letter-spacing -0.05em
    color #001431
    text-decoration underline
.creation
  +element(holder)
    display flex
    align-items center
  +element(text)
    font-weight 400
    font-size 10px
    letter-spacing -0.05em
    color #7A7A7A
    margin-right 8px
  +element(info)
    font-weight 900
    font-size 11px
    letter-spacing -0.05em
    color #000
    margin-left 6px
  +element(circle)
    position relative
    width 38px
    height 38px
    background #000
    border-radius 50%
  +element(circle-inner)
    position absolute
    top 50%
    left 50%
    transform translate(-50%, -50%)
    width 28px
    height 28px
    background #fff
    border-radius 50%
  +element(circle-inner-black)
    position absolute
    top 50%
    left 50%
    transform translate(-50%, -50%)
    width 18px
    height 18px
    background #000
    border-radius 50%